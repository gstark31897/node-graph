<NodeNumericInput>:
    size: 100, 20
    canvas:
        Color:
            rgb: [0.5, 0.8, 0.5]
        Ellipse:
            size: 20, 20
            pos: -10, 10
    BoxLayout:
        orientation: 'vertical'
        Label:
            text: 'Frequency'
            color: [0, 0, 0, 1]
        TextInput

<NodeWidget>: 
    size: 200, 200
    canvas: 
        Color: 
            rgb: [0.6, 0.6, 0.6] 
        Rectangle: 
            size: self.size 
            pos: 0, 0
    BoxLayout:
        width: self.parent.width
        height: self.parent.height - 20
        orientation: 'horizontal'
        BoxLayout:
            id: inputs
            orientation: 'vertical'
        BoxLayout:
            id: outputs
            orientation: 'vertical'
